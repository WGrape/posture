#!/bin/sh
currentDir=$(pwd)
. $currentDir/include/function.sh
COMMAND=$1

if [ "${COMMAND}" == "hooking" ]; then
  hooking $currentDir
elif [ "${COMMAND}" == "adjust" ]; then
  checkConfig && adjust
elif [ "${COMMAND}" == "info" ]; then
  echo "The list of information"
  echo "> lang = ${lang}"
else
  echo "Unknown command: ${COMMAND}"
fi
